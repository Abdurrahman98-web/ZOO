Options +FollowSymLinks
RewriteEngine On

# Klasöre erişimi engelleme (ihtiyacınıza göre kaldırabilirsiniz)
<Directory "/">
    AllowOverride All
    Require all granted 
    <FilesMatch "\.(php|html|htm|css|js)$">
    Require all granted
    </FilesMatch>
    <FilesMatch "^\.DS_Store$">
    Require all denied
</FilesMatch>


</Directory>

